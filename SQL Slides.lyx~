\end_layout

\begin_layout Plain Layout

    ->     FROM account a 
\end_layout

\begin_layout Plain Layout

    ->     WHERE a.cust_id = c.cust_id) AS balance 
\end_layout

\begin_layout Plain Layout

    -> FROM customer AS c;
\end_layout

